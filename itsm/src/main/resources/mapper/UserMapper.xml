<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.mof.cc.itsm.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.mof.cc.itsm.entity.User">
        <id column="id" property="id" />
        <result column="username" property="username" />
        <result column="fullname" property="fullname" />
        <result column="password" property="password" />
        <result column="isused" property="isused" />
        <result column="create_time" property="createTime" />
        <result column="modify_time" property="modifyTime" />
        <result column="bak1" property="bak1" />
        <result column="bak2" property="bak2" />
        <result column="bak3" property="bak3" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, username, fullname, password, isused, create_time, modify_time, bak1, bak2, bak3
    </sql>

</mapper>

